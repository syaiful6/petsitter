{
  "name": "syaiful6/skellie",
  "description": "skellie app",
  "homepage": "https://github.com/syaiful6/skellie",
  "type": "library",
  "keywords": ["skeleton"],
  "license": "MIT",
  "authors": [
    {
      "name": "Syaiful Bahri",
      "email": "syaiful@pengunjungblog.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "syaiful6/pawon": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "filp/whoops": "^1.1 || ^2.0",
    "mockery/mockery": "~0.9.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/syaiful6/pawon.git"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev"
}
